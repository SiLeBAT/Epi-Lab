{
  "name": "mibi-portal-client",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "ng serve --proxy-config proxy.config.json --watch",
    "debug": "ng serve -o --proxy-config proxy.config.json --watch",
    "serve:local": "ng serve",
    "dependency-check:production": "npx npm-auditor-ci --threshold low --ignoreDev",
    "dependency-check:qa": "npx npm-auditor-ci --threshold low",
    "build:qa": "ng build --configuration=qa --output-path \"public\"",
    "prebuild:qa": "npm run clean",
    "build:production": "ng build --configuration=production --output-path \"public\"",
    "prebuild:production": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:e2e": "node_modules\\.bin\\cypress run",
    "reports:delete": "npx rimraf  cypress/reports/* || true",
    "reports:create": "npx mochawesome-merge --reportDir cypress/reports/mocha > cypress/reports/mocha/merged-report.json && npx mochawesome-report-generator cypress/reports/mocha/merged-report.json -o cypress/reports/mocha -t \"mibi-portal e2e\" -p \"mibi-portal e2e\" --charts --timestamp \"isoDateTime\" --showSkipped",
    "pretest:e2e": "npm run reports:delete",
    "posttest:e2e": "npm run reports:create",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "tslint --project tsconfig.json --fix -c tslint.json 'src/**/*.ts'",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "clean": "npx rimraf public/*",
    "cypress:open": "node_modules\\.bin\\cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:code",
      "git add"
    ],
    "*.scss": [
      "npm run lint:styles",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@auth0/angular-jwt": "^2.1.2",
    "@handsontable/angular": "^2.0.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "angular-file": "^0.5.9",
    "angular-password-strength-meter": "^0.0.4",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.6.9",
    "cypress-multi-reporters": "^1.2.1",
    "file-saver": "^1.3.8",
    "handsontable": "^4.0.0",
    "html5-history-api": "^4.2.10",
    "jquery": "^3.4.1",
    "linebyline": "^1.3.0",
    "lodash": "^4.17.15",
    "mocha-junit-reporters": "^1.23.6",
    "moment": "^2.24.0",
    "ngx-moment": "^3.4.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.5.3",
    "tooltipster": "^4.2.7",
    "ts-xlsx": "^0.0.11",
    "xlsx": "^0.13.5",
    "xlsx-populate": "^1.20.1",
    "zone.js": "0.9.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@ngrx/schematics": "^6.1.2",
    "@types/core-js": "^2.5.2",
    "@types/file-saver": "^1.3.1",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.139",
    "@types/node": "^10.14.19",
    "@types/tooltipster": "0.0.29",
    "@types/xlsx": "^0.0.36",
    "@types/zxcvbn": "^4.4.0",
    "codelyzer": "^4.5.0",
    "cypress": "^3.4.1",
    "cypress-file-upload": "^3.3.4",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^6.0.2",
    "lint-staged": "^8.2.1",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "rxjs-tslint": "^0.1.7",
    "rxjs-tslint-rules": "^4.25.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "3.2.4"
  },
  "mibiConfig": {
    "lastChange": "2019-04-16 11:25:17 +0200"
  }
