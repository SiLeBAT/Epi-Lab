{
  "name": "mibi-portal-client",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "ng serve --proxy-config proxy.config.json --watch",
    "serve:local": "ng serve",
    "build:qa": "npm run clean && ng build --configuration=qa --output-path \"public\"",
    "build:prod": "npm run clean && ng build --configuration=production --output-path \"public\"",
    "dependency-check:prod": "node_modules/.bin/npm-audit-ci-wrapper -t low -p",
    "dependency-check:qa": "node_modules/.bin/npm-audit-ci-wrapper -t low",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "tslint --project tsconfig.json --fix -c tslint.json 'src/**/*.ts'",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "e2e": "ng e2e",
    "clean": "rimraf public/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./hooks/commit-msg ./.git/COMMIT_EDITMSG"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:code",
      "git add"
    ],
    "*.scss": [
      "npm run lint:styles",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@auth0/angular-jwt": "^2.0.0",
    "@handsontable/angular": "^2.0.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "angular-file": "^0.5.4",
    "angular-password-strength-meter": "^0.0.4",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.5.7",
    "file-saver": "^1.3.8",
    "hammerjs": "^2.0.8",
    "handsontable": "^4.0.0",
    "html5-history-api": "^4.2.10",
    "jquery": "^3.3.1",
    "linebyline": "^1.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ngx-moment": "^3.2.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.3.3",
    "tooltipster": "^4.2.6",
    "ts-xlsx": "^0.0.11",
    "xlsx": "^0.13.5",
    "xlsx-populate": "^1.17.0",
    "zone.js": "^0.8.26",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^6.1.10",
    "@ngrx/schematics": "^6.1.2",
    "@types/core-js": "^2.5.0",
    "@types/file-saver": "^1.3.1",
    "@types/jquery": "^3.3.22",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.2",
    "@types/tooltipster": "0.0.29",
    "@types/xlsx": "^0.0.36",
    "@types/zxcvbn": "^4.4.0",
    "codelyzer": "^4.5.0",
    "husky": "^1.1.3",
    "jest": "^24.1.0",
    "jest-preset-angular": "^6.0.1",
    "lint-staged": "^8.0.4",
    "npm-audit-ci-wrapper": "^1.0.8",
    "rimraf": "^2.6.2",
    "rxjs-tslint": "^0.1.5",
    "rxjs-tslint-rules": "^4.19.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "3.1.1"
  },
  "mibiConfig": {
    "lastChange": "Mon Sep 17 11:21:33 2018 +0200"
  }
}
